{
  "name": "@tanstack/react-router-server",
  "version": "1.7.1",
  "description": "",
  "author": "Tanner Linsley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TanStack/router.git",
    "directory": "packages/history"
  },
  "homepage": "https://tanstack.com/react-router-server",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "type": "module",
  "exports": {
    "./client": {
      "import": {
        "types": "./dist/esm/client.d.ts",
        "default": "./dist/esm/client.js"
      },
      "require": {
        "types": "./dist/cjs/client.d.cts",
        "default": "./dist/cjs/client.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/esm/server.d.ts",
        "default": "./dist/esm/server.js"
      },
      "require": {
        "types": "./dist/cjs/server.d.cts",
        "default": "./dist/cjs/server.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./coverage",
    "test:types": "tsc",
    "test:build": "publint --strict",
    "build": "vite build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "react",
    "location",
    "router",
    "routing",
    "async",
    "async router",
    "typescript"
  ],
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@tanstack/react-cross-context": "workspace:^",
    "@tanstack/react-router": "workspace:*",
    "isbot": "^3.6.6",
    "jsesc": "^3.0.2",
    "tiny-invariant": "^1.3.1",
    "tiny-warning": "^1.0.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1"
  }
}
